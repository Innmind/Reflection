{
    "name": "innmind/reflection",
    "type": "library",
    "description": "Library to build objects and extract data out of them",
    "keywords": ["reflection"],
    "homepage": "http://github.com/Innmind/Reflection",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "baptiste.langlade@hey.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/Reflection/issues"
    },
    "require": {
        "php": "~8.1",
        "innmind/immutable": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Reflection\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Reflection\\": "tests/",
            "Fixtures\\Innmind\\Reflection\\": "fixtures/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "vimeo/psalm": "~4.22",
        "innmind/coding-standard": "~2.0",
        "innmind/black-box": "^4.18"
    }
}
